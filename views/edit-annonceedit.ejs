<main>

    <form id="annonceedit" method="post" class="form-inline">
    <div class="form-group">
        <label for="navn">Navn</label>
        <input type="text" name="navn" class="form-control" id="navn" value="<%= 
            (typeof(mainResults) != 'undefined' && typeof(mainResults.navn) != 'undefined') ? mainResults.navn : '' %>">
        <label for="navn">Adresse</label>
        <input type="text" name="adresse" class="form-control" id="adresse" value="<%= 
        (typeof(mainResults) != 'undefined' && typeof(mainResults.adresse) != 'undefined') ? mainResults.adresse : '' %>">
        <label for="navn">Pladser</label>
        <input type="text" name="pladser" class="form-control" id="pladser" value="<%= 
        (typeof(mainResults) != 'undefined' && typeof(mainResults.pladser) != 'undefined') ? mainResults.pladser : '' %>">
        <label for="navn">Billede</label>
        <input type="text" name="billede" class="form-control" id="billede" value="<%= 
        (typeof(mainResults) != 'undefined' && typeof(mainResults.billede) != 'undefined') ? mainResults.billede : '' %>">
        <label for="navn">Pris</label>
        <input type="text" name="pris" class="form-control" id="pris" value="<%= 
        (typeof(mainResults) != 'undefined' && typeof(mainResults.pris) != 'undefined') ? mainResults.pris : '' %>">
        <button type="submit">Gem</button>
    </div>
</main>